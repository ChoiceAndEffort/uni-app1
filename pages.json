{
		"easycom": {
			"^u-(.*)": "uview-ui/components/u-$1/u-$1.vue"
		},
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/index/index",
			"style": {
					"navigationStyle": "custom"
			}
		},
		{
			"path": "pages/tabBar/beautiful/index",
			"style": {
					"navigationBarTitleText": "美景"
			}
		},
		{
			"path": "pages/tabBar/foods/index",
			"style": {
				"navigationBarTitleText": "美食"
			}
		},
		{
			"path": "pages/tabBar/animal/index",
			"style": {
				"navigationBarTitleText": "我的车"
			}
		},
		{
			"path": "pages/tabBar/me/index",
			"style": {
				"navigationBarTitleText": "我的",
				"enablePullDownRefresh": false
			}
		}
	],
	"tabBar": {
    "color": "#7A7E83",
    "selectedColor": "#3cc51f",
    "borderStyle": "black",
    "backgroundColor": "#ffffff",
		"list": [{
				"pagePath": "pages/tabBar/beautiful/index",
				"iconPath": "static/tab/img.png",
				"selectedIconPath":"static/tab/img.png",
				"text": "美景"
			},
			{
				"pagePath": "pages/tabBar/foods/index",
				"iconPath":"static/tab/huo.png",
				"selectedIconPath":"static/tab/huo.png",
				"text": "美食"
			},
			{
				"pagePath": "pages/tabBar/animal/index",
				"iconPath": "static/tab/niu.png",
				"selectedIconPath": "static/tab/niu.png",
				"text": "车"
			},
			{
				"pagePath": "pages/tabBar/me/index",
				"iconPath": "static/tab/me.png",
				"selectedIconPath": "static/tab/me.png",
				"text": "我的"
			}
		]
	},
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	}
}
